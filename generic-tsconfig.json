{
  "compilerOptions": {
    "target": "ES2019",
    "downlevelIteration": true,
    "module": "esnext",
    "strict": true,
    "moduleResolution": "node",
    "outDir": "./build",
    "tsBuildInfoFile": ".ts-tmp/.tsconfig.tsbuildinfo",
    "composite": true,
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "sourceMap": false
  }
}
